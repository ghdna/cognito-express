{
   "name": "cognito-express",
   "version": "2.0.17",
   "description": "cognito-express authenticates API requests on a Node-Express application by verifying the signature of AccessToken or IDToken generated by Amazon Cognito.",
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "scripts": {
      "test": "ts-mocha test/**/*.test.ts --timeout 10000",
      "test:coverage": "nyc --reporter=lcovonly npm test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
      "start": "npm test",
      "prepare": "npx tsc"
   },
   "keywords": [
      "express",
      "aws",
      "amazon",
      "passport",
      "cognito",
      "auth",
      "authentication",
      "identity"
   ],
   "author": "Gary Arora (https://twitter.com/AroraGary)",
   "license": "MIT",
   "files": [
      "LICENSE",
      "README.md",
      "lib/"
   ],
   "dependencies": {
      "jsonwebtoken": "^8.3.0",
      "jwk-to-pem": "^2.0.0",
      "request": "^2.88.0",
      "request-promise": "^4.2.2"
   },
   "devDependencies": {
      "@types/expect": "^24.3.0",
      "@types/mocha": "^8.0.3",
      "chai": "^4.2.0",
      "chai-as-promised": "^7.1.1",
      "coveralls": "^3.0.2",
      "istanbul": "^0.4.5",
      "mocha": "^8.0.1",
      "nyc": "^15.1.0",
      "rewire": "^4.0.1",
      "ts-mocha": "^8.0.0",
      "typescript": "^4.0.5"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/ghdna/cognito-express.git"
   },
   "bugs": {
      "url": "https://github.com/ghdna/cognito-express/issues"
   },
   "homepage": "https://github.com/ghdna/cognito-express#readme"
}
