{
  "name": "cognito-express",
  "version": "2.0.19",
  "description": "cognito-express authenticates API requests on a Node-Express application by verifying the signature of AccessToken or IDToken generated by Amazon Cognito.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' nyc --reporter=text mocha --require ts-node/register --extensions ts,tsx 'test/**/*.{ts,tsx}'",
    "start": "npm test",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "build": "tsc"
  },
  "keywords": [
    "express",
    "aws",
    "amazon",
    "passport",
    "cognito",
    "auth",
    "authentication",
    "identity"
  ],
  "author": "Gary Arora (https://twitter.com/AroraGary)",
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "node-fetch": "^2.6.6"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.16",
    "@types/chai": "^4.2.22",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node-fetch": "^2.5.12",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "rewire": "^5.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghdna/cognito-express.git"
  },
  "bugs": {
    "url": "https://github.com/ghdna/cognito-express/issues"
  },
  "homepage": "https://github.com/ghdna/cognito-express#readme"
}
